Package: pseudorank
Title: Pseudo-Ranks
Version: 0.1.0
Date: 2018-06-08
Authors@R: c(person("Martin Happ", role = c("aut", "cre"),
                      email = "martin.happ@aon.at",
                      comment = c(ORCID = "0000-0003-0009-2665")),
	      person("Georg Zimmermann", role = "aut"),
	      person("Arne C. Bathke", role = "aut"),
